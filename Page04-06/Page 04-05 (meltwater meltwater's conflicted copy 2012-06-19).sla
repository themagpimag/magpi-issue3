            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="Whilst playing press 'p' to pause 'q' to quit. Arrows to jump forward/back."/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow (ArialNarrow-Bold)" FONTSIZE="12" CH="INSTALLING CHROMIUM WEB BROWSER"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="Chromium is slow on the Pi, but it works well with the following useful websites:"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="http://qwebirc.swiftirc.net - instant messenger chat rooms"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="http://www.dropbox.com - upload/download/share/transfer"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="http://www.gmail.com in Basic HTML mode - send/receive google email with attachments"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="sudo apt-get install chromium-browser"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow (ArialNarrow-Bold)" FONTSIZE="12" CH="INSTALLING QUAKE III GAME"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="From the command prompt or LXTerminal window type:"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="cd ~"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="wget http://radium.hexxeh.net/quake3.zip"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="wget http://www.andershizzle.com/Q3%20Demo%20Paks.zip"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="unzip quake3.zip"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="unzip &quot;Q3 Demo Paks.zip&quot;"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="rm quake3.zip"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="rm &quot;Q3 Demo Paks.zip&quot;"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="mv ./baseq3/pak* ./quake3/baseq3/"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="rm -rf ./baseq3"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="chmod +x /home/pi/quake3/start.sh"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="chmod +x /home/pi/quake3/ioquake3.arm"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="chmod +x /home/pi/quake3/ioq3ded.arm"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="To start Quake III type at the command prompt:"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="cd ~/quake3/"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" FCOLOR="Blue" FSHADE="100" CH="./start.sh"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow (ArialNarrow-Bold)" FONTSIZE="12" CH="INSTALLING XPDF"/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="XPDF is a free PDF document viewer."/>
            <para LINESP="15"/>
            <ITEXT FONT="Arial Narrow" FONTSIZE="11" CH="Download The MagPi pdf's by opening Midori web browser, and visiting http://www.themagpi.com.  Click on one of the PDF download mirrors.  Choose 'Save' and watch the progress of the download at the bottom right of the browser."/>
