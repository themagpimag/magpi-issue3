            <ITEXT FONT="FreeSans Bold" FONTSIZE="12" CH="The Tricky Bit"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH=" - Making connections"/>
            <para/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="10" CH="I usually use a small bench vice for this, but failing that, a pair of pliers or molegrips will have to suffice."/>
            <trail/>
            <PageItemAttributes/>
        </PAGEOBJECT>
